/*START HEADER*/
header
{
	position: absolute;
	width: 100%;
	vertical-align: top;
    padding-top: 20px;
    z-index: 10;
    &.internal-pages
    {
        position: relative;
        
        .menu-buttom
        {
            display: block;
            margin-top: 70px;
            margin-left: 0;
            
            li
            {
                a
                {
                    color: #000;
                }
                &.active
                {
                    a
                    {
                        color: #ff5722;
                    }
                }
            }
        }
    }
    .menu-buttom
    {
        display: inline-block;
        margin-left: 30px;
        
        li
        {
            display: inline-block;
            
            &:after
            {
                content: '/';
                margin: 0 5px 0 10px;
                color: #000;
            }
            &:last-of-type
            {
                &:after
                {
                    display: none;
                }
            }
            a
            {
                font-size: 14px;
                font-weight: 400;
                text-transform: uppercase;
                color: #fff;
            }
            .active
            {
                a
                {
                    color: #ff5722;
                }
            }
        }
    }
    .menu li.active
    {
        a:before 
        {
            visibility: visible;
            width: 100%;
            left: 0;
        }
    }
    .menu li a
    {
        position: relative;
        
        &:before 
        {
            content: '';
            background-color: #fff;
            height: 2px;
            width: 10px;
            bottom: -5px;
            left: 45%;
            visibility: hidden;
            position: absolute;
            transition: 0.2s ease all;
            -moz-transition: 0.2s ease all;
            -webkit-transition: 0.2s ease all;
        }
        &:hover:before 
        {
            visibility: visible;
            width: 100%;
            left: 0;
        }
    }
    &.internal-pages
    {
        &+section
        {
            margin-top: 90px;
        }
    }
    &.internal-pages
    {
        .menu li a
        {
            color: #000 !important;
        }
        li a:before
        {
            background: #000 !important;
        }    
        
        
    }
	.logo
	{
		display: inline-block;
		vertical-align: middle;
		margin-left: 0px;
		width: 210px;
		height: 70px;
        margin-top: -5px;
		
		img
		{
			max-width: 100%;
		}
	}
	

	.menu
	{

		text-align: right;
		line-height: 40px;
		vertical-align: top;
        margin: 25px auto 0;
        position: fixed;
        z-index: 100;
        top: 0px;
        left: 0;
        display: table;
        right: -840px;
        
        input
        {
            height: 20px;
            color: #333;
            content: '';
            z-index: -1;
            -webkit-transform-origin: 100% 50%;
            transform-origin: 100% 50%;
            -webkit-transform: scale3d(0, 1, 1);
            transform: scale3d(0, 1, 1);
            -webkit-transition: -webkit-transform 0.5s, opacity 0.3s;
            transition: transform 0.5s, opacity 0.3s;
            -webkit-transition-timing-function: cubic-bezier(0.7,0,0.9,1);
            transition-timing-function: cubic-bezier(0.7,0,0.9,1);
            position: absolute;
            right: 38px;
            top: 7px;
        }
        .search
        {
            position: relative;
        }
        .active
        {
            background-color:  #dd2c00;
            input
            {
                -webkit-transform: scale3d(1, 1, 1);
                transform: scale3d(1, 1, 1);
                position: relative;
                right: inherit;
                top: inherit;
                z-index: 1;
            }
        }
        li
        {
            margin-left: -5px;
            display: inline-block;
            background-color:  #ff5722;
            color: #fff;
            padding: 0 15px;
            text-transform: uppercase;
            cursor: pointer;
            
            &:last-of-type
            {
                i:before
                {
                    margin-right: 0 !important;
                }
            }
            
            &:hover
            {
                background-color:  #dd2c00;
            }
            
            i:before
            {
                vertical-align: middle;
                font-size: 20px;
                color: #fff;
                margin-right: 9px;
            }
        }
		
	}
}
