            &[type="text"]
            {
                width: 380px;
                margin-right: 10px;
            }
            &[type="tel"]
            {
                width: 245px;
                margin-left: 14px;
            }
            &[type="email"]
            {
                width: 460px;
                margin-left: 40px;
            }
        }
        input ~ label:after {
            content: '';
            background-color: #ff6600;
            height: 2px;
            width: 10px;
            bottom: 0;
            left: 45%;
            visibility: hidden;
            position: absolute;
            transition: 0.2s ease all;
            -moz-transition: 0.2s ease all;
            -webkit-transition: 0.2s ease all;
        }
        input:focus ~ label:after 
        {
            visibility: visible;
            width: 100%;
            left: 0;
        }
    }
